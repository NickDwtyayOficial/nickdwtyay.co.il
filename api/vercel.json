{
  "version": 2,
  "rewrites": [
    {
      "source": "/register.php",
      "destination": "/api/register.php"
    },
    {
      "source": "/profile.php",
      "destination": "/api/profile.php"
    },
    {
      "source": "/logout.php",
      "destination": "/api/logout.php"
    },
    {
      "source": "/(.*)",
      "destination": "/api/index.php"
    }
  ],
  "functions": {
    "api/*.php": {
      "runtime": "vercel-php@0.7.3"
    }
  }
}
